# Twilio (SMS)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+15555555555
# Optional flag to disable Twilio in non-prod (set to 'false' to disable)
TWILIO_ENABLED=true

# SendGrid (Email)
SENDGRID_API_KEY=your_sendgrid_api_key
# Optional flag to disable SendGrid in non-prod (set to 'false' to disable)
SENDGRID_ENABLED=true

# Notes:
# - Do NOT commit real secrets. Use your environment/SSM/secret manager in CI/production.
# - For local development, set TWILIO_ENABLED=false to avoid sending real SMS.
# Application Environment
NODE_ENV=development
PORT=3000

# MySQL Database Configuration
DB_HOST=your-mysql-host.amazonaws.com
DB_PORT=3306
DB_USERNAME=your_username
DB_PASSWORD=your_password
DB_DATABASE=your_database_name